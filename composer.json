  "homepage": "https://github.com/arkhi/grav-plugin-json-ld-breadcrumbs",
  "license": "MIT",
  "authors": [
    {
      "name": "Fabien Basmaison",
      "email": "contact@arkhi.org",
      "role": "Developer"
    }
